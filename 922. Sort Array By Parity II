
class Solution {
public:
    vector<int> sortArrayByParityII(vector<int>& nums) {
         vector<int>e;
         vector<int>o;
         for(int i=0;i<nums.size();i++)
         {
            if(nums[i]%2==0)
            e.push_back(nums[i]);
            else
            o.push_back(nums[i]);
         }
         vector<int>ans;
         for(int i=0;i<e.size();i++)
         {
            ans.push_back(e[i]);
            ans.push_back(o[i]);
         }
        return
    }
};
