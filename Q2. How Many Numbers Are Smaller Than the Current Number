class Solution {
public:
    vector<int> smallerNumbersThanCurrent(vector<int>& nums) {
        vector<int>temp(nums);
        sort(nums.begin(),nums.end());
        map<int,int>mp;
        for(int i=1;i<nums.size();i++){
            if(nums[i]!=nums[i-1]){
                mp[nums[i]]=i;
            }
        }
        for(int i=0;i<temp.size();i++){
            temp[i]=mp[temp[i]];

        }   
        return temp;
    }
};
