class Solution {
public:
    int secondHighest(string s) {
        int high=0,sec=0;
        vector<int>v;
        for(int i=0;i<s.size();i++){
            if(isdigit(s[i])){
                int x=s[i]-'0';
                v.push_back(x);
            }
        }
        sort(v.begin(),v.end());
        for(int i=v.size()-1;i>0;i--){
            if(v[i]!=v[i-1])
            return v[i-1];
        }
        return -1;
    }
};
